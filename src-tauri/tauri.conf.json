{
  "productName": "chat-ezbox",
  "version": "0.0.1",
  "identifier": "com.tauri.dev",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:14620",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {"windows": [
      {
        "title": "Chat Ezbox",
        "width": 420,
        "height": 960,
        "minWidth": 320,
        "minHeight": 600,
        "hiddenTitle": true,
        "contentProtected": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
